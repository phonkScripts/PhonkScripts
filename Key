local KeySystemUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/MaGiXxScripter0/keysystemv2api/master/ui/xrer_mstudio45.lua"))()
KeySystemUI.New({
    ApplicationName = "PhonkScripts", -- Your Key System Application Name
    Name = "Phonk Script's", -- Your Script name
    Info = "Join discord to get more news about scripts", -- Info text in the GUI, keep empty for default text.
    DiscordInvite = "https://discord.gg/uubD6jHH9F" -- Optional.
})
repeat task.wait() until KeySystemUI.Finished() or KeySystemUI.Closed
if KeySystemUI.Finished() and KeySystemUI.Closed == false then
   loadstring(game:HttpGet("https://raw.githubusercontent.com/phonkScripts/PhonkScripts/main/game%20protect"))()
   print("key is valid")
else
    print("Player closed the GUI.")
end

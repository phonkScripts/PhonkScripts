local Button = Tab2:CreateButton({
	Name = "InfJump [NOT WORKING]",
	Callback = function()

local function TKDWQ_fake_script() -- INFJUMP.LocalScript 
local script = Instance.new('LocalScript', INFJUMP)

function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end

counter = 0

while wait(0.1)do
script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
 
counter = counter + 0.01
end
end
coroutine.wrap(TKDWQ_fake_script)()
	end,
})

local Button = Tab2:CreateButton({
	Name = "noclip [Press 'N']",
	Callback = function()

local player = game.Players.LocalPlayer -- Получаем локального игрока
local character = player.Character -- Получаем персонаж игрока

local function enableNoclip()
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CanCollide = false -- Отключаем коллизии для всех частей персонажа
        end
    end
end

local function disableNoclip()
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CanCollide = true -- Включаем коллизии для всех частей персонажа
        end
    end
end

local noclipEnabled = false

game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessedEvent)
    if not gameProcessedEvent then
        if input.KeyCode == Enum.KeyCode.N then -- Можете использовать другую клавишу по желанию
            noclipEnabled = not noclipEnabled -- Инвертируем состояние noclip
            if noclipEnabled then
                enableNoclip()
            else
                disableNoclip()
            end
        end
    end
end)
end,
})
